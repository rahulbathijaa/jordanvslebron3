"builds": [
    {
        "src": "backend/app.py",
        "use": "@vercel/python"
    },
    {
        "src": "package.json",
        "use": "@vercel/node"
      }
],
"routes": [
    {
        "src": "/api/(.*)",
        "dest": "/backend/app.py"
    }
    {
        "src": "/(.*)",
        "dest": "/"
      }
]